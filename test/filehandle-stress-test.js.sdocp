sdocp('test/filehandle-stress-test.js.sdoc', 'Test for filehandle exhaustion and general performance with small updates\n\ncaterwaul.clone(\'std\', \'error\', \'iter\', \'db.file\')(function () {\n  var db = this[caterwaul].db.file(\'test-db\');\n  console.log(\'single record, many updates\');\n  iter.n[i, 10000][db(\'record\')({foo: i})];\n  \n  console.log(\'many records, one update per\');\n  iter.n[i, 10000][db(\'record#{i}\')({bar: i})];\n})();\n');